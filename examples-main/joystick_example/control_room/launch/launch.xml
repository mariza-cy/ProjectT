<launch>

    <arg name="veh" description="Name of vehicle. ex: megaman" default="$(env VEHICLE_NAME)"/>
    <arg name="user" description="Name of user of the node" default="$(env USER_NAME)"/>
    <node pkg="control_room" exec="main.py" name="control_room_$(var user)" output="screen">
<!--        emulate_tty="true" launch-prefix="bash -c 'exec < /dev/tty &amp;&amp; exec $0 $@'"-->
        <param name="vehicle_name" value="$(var veh)"/>
    </node>

</launch>